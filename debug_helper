from robot.api import logger
from robot.libraries.BuiltIn import BuiltIn


def get_browser_kwargs():
    apprun = BuiltIn().get_variable_value("${apprun}", default="web")
    web_url = BuiltIn().get_variable_value("${WEB_URL}", default="")
    electron_app = BuiltIn().get_variable_value("${ELECTRON_APP}", default="")

    # Console logs
    BuiltIn().log_to_console(f"📦 apprun: {apprun}")
    BuiltIn().log_to_console(f"🌐 WEB_URL: {web_url}")
    BuiltIn().log_to_console(f"🧪 ELECTRON_APP: {electron_app}")

    # Report logs
    logger.info(f"📦 apprun: {apprun}")
    logger.info(f"🌐 WEB_URL: {web_url}")
    logger.info(f"🧪 ELECTRON_APP: {electron_app}")

    return {
        "apprun": apprun,
        "WEB_URL": web_url,
        "ELECTRON_APP": electron_app
    }
